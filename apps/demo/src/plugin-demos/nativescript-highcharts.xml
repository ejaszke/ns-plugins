<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page"
      xmlns:ui="@imagene.me/nativescript-highcharts">
    <Page.actionBar>
        <ActionBar title="nativescript-highcharts" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout class="p-20">
        <ScrollView class="h-full">
          <StackLayout class="h-full">
            <ui:Highcharts options="{{ chartOptionsString }}"></ui:Highcharts>
          </StackLayout>
        </ScrollView>
    </StackLayout>
</Page>
